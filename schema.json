{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "page",
    "per_page",
    "total",
    "total_pages",
    "data"
  ],
  "properties": {
    "page": {
      "$id": "#/properties/page",
      "type": "integer",
      "title": "The Page Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "per_page": {
      "$id": "#/properties/per_page",
      "type": "integer",
      "title": "The Per_page Schema",
      "default": 0,
      "examples": [
        6
      ]
    },
    "total": {
      "$id": "#/properties/total",
      "type": "integer",
      "title": "The Total Schema",
      "default": 0,
      "examples": [
        12
      ]
    },
    "total_pages": {
      "$id": "#/properties/total_pages",
      "type": "integer",
      "title": "The Total_pages Schema",
      "default": 0,
      "examples": [
        2
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "The Data Schema",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "name",
          "year",
          "color",
          "pantone_value"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/data/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "name": {
            "$id": "#/properties/data/items/properties/email",
            "type": "string",
            "title": "The name Schema",
            "default": "",
            "examples": [
              "cerulean"
            ],
            "pattern": "^(.*)$"
          },
          "year": {
            "$id": "#/properties/data/items/properties/first_name",
            "type": "integer",
            "title": "The year Schema",
            "default": 0,
            "examples": [
              2000
            ]
          },
          "color": {
            "$id": "#/properties/data/items/properties/last_name",
            "type": "string",
            "title": "The Last_name Schema",
            "default": "",
            "examples": [
              "#98B2D1"
            ],
            "pattern": "^#(.*)$"
          },
          "pantone_value": {
            "$id": "#/properties/data/items/properties/avatar",
            "type": "string",
            "title": "The Avatar Schema",
            "default": "",
            "examples": [
              "15-4020"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}